<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="www.newproj.com.newpro.FacewomenActivity"
    android:background="@color/black_overlay"
    android:orientation="vertical"
    android:gravity="top"
    android:padding="10dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_margin="5dp"
            android:text="WOMEN FACE STRUCTURE"
            android:textColor="@color/backgroundcolor"
            android:textSize="25sp"
            android:textStyle="bold" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:gravity="center"
        android:orientation="horizontal">

        <android.support.v7.widget.CardView
            android:layout_width="160dp"
            android:layout_height="140dp"
            android:layout_margin="9dp"
            android:id="@+id/ovalwomenfacestructure"
            android:onClick="onClick">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="160dp"
                    android:layout_height="120dp"
                    android:background="@color/black_overlay"
                    android:padding="10dp"
                    android:src="@drawable/ovalshapegirl" />
                <TextView
                    android:id="@+id/ovalwomenView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="OVAL"
                    android:visibility="visible"
                    android:textColor="@color/black_overlay"
                    android:gravity="center"/>


            </LinearLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="160dp"
            android:layout_height="140dp"
            android:layout_margin="9dp"
            android:id="@+id/roundwomenfacestructure"
            android:onClick="onClick">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="160dp"
                    android:layout_height="120dp"
                    android:background="@color/black_overlay"
                    android:padding="10dp"
                    android:src="@drawable/round" />
                <TextView
                    android:id="@+id/roundwomenView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="ROUND"
                    android:visibility="visible"
                    android:textColor="@color/black_overlay"
                    android:gravity="center"/>


            </LinearLayout>

        </android.support.v7.widget.CardView>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:gravity="center"
        android:orientation="horizontal">

        <android.support.v7.widget.CardView
            android:layout_width="160dp"
            android:layout_height="140dp"
            android:layout_margin="9dp"
            android:id="@+id/heartwomenfacestructure"
            android:onClick="onClick">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="160dp"
                    android:layout_height="120dp"
                    android:background="@color/black_overlay"
                    android:padding="10dp"
                    android:src="@drawable/heart" />
                <TextView
                    android:id="@+id/heartwomenView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="HEART"
                    android:visibility="visible"
                    android:textColor="@color/black_overlay"
                    android:gravity="center"/>


            </LinearLayout>

        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:layout_width="160dp"
            android:layout_height="140dp"
            android:layout_margin="9dp"
            android:id="@+id/longwomenfacestructure"
            android:onClick="onClick">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="160dp"
                    android:layout_height="120dp"
                    android:background="@color/black_overlay"
                    android:padding="10dp"
                    android:src="@drawable/longshape" />
                <TextView
                    android:id="@+id/longshapewomenView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="LONGSHAPE"
                    android:visibility="visible"
                    android:textColor="@color/black_overlay"
                    android:gravity="center"/>


            </LinearLayout>

        </android.support.v7.widget.CardView>


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:gravity="center"
        android:orientation="horizontal">

        <android.support.v7.widget.CardView
            android:layout_width="160dp"
            android:layout_height="140dp"
            android:layout_margin="9dp"
            android:id="@+id/squarewomenfacestructure"
            android:onClick="onClick">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="160dp"
                    android:layout_height="120dp"
                    android:background="@color/black_overlay"
                    android:padding="10dp"
                    android:src="@drawable/square" />
                <TextView
                    android:id="@+id/squarewomenView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="SQUARE"
                    android:visibility="visible"
                    android:textColor="@color/black_overlay"
                    android:gravity="center"/>



            </LinearLayout>

        </android.support.v7.widget.CardView>

    </LinearLayout>



</LinearLayout>
